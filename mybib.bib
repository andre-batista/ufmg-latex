@article{bao2007inverse,
	title = {Inverse scattering by a continuation method with initial guesses from a direct imaging algorithm},
	journal = {Journal of Computational Physics},
	volume = {227},
	number = {1},
	pages = {755-762},
	year = {2007},
	issn = {0021-9991},
	doi = {https://doi.org/10.1016/j.jcp.2007.08.020},
	url = {https://www.sciencedirect.com/science/article/pii/S0021999107003567},
	author = {Gang Bao and Songming Hou and Peijun Li},
	keywords = {Continuation method, MUSIC, Inverse scattering},
	abstract = {A novel continuation method is presented for solving the inverse medium scattering problem of the Helmholtz equation, which is to reconstruct the shape of the inhomogeneous medium from boundary measurements of the scattered field. The boundary data is assumed to be available at multiple frequencies. Initial guesses are chosen from a direct imaging algorithm, multiple signal classification (MUSIC), along with a level set representation at a certain wavenumber, where the Born approximation may not be valid. Each update via recursive linearization on the wavenumbers is obtained by solving one forward and one adjoint problem of the Helmholtz equation.}
}

